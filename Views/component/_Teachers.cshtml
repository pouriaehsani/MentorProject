@using mentorproject.Models
@model GeneralModelView

@{
    foreach (View_trainers item in Model.Teachers)
    {
        if (item.firstname != "استاد")
        {
            <div class="col-lg-4 col-md-6 d-flex" data-aos="fade-up" data-aos-delay="100">
                <div class="member">
                    <img src="~/AdminAssets/img/ImgTeacher/@item.address" class="img-fluid" title="@item.title" alt="@item.alt" style="width:350px;height:350px">
                    <div class="member-content">
                        <h4>@item.firstname @item.lastname</h4>
                        <span></span>

                        <div class="social">
                            <a href=""><i class="bi bi-twitter-x"></i></a>
                            <a href=""><i class="bi bi-facebook"></i></a>
                            <a href=""><i class="bi bi-instagram"></i></a>
                            <a href=""><i class="bi bi-linkedin"></i></a>
                        </div>
                    </div>
                </div>
            </div><!-- End Team Member -->
        }

    }
}
